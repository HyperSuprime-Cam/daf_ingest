    scienceExposure: {
        Type: "ExposureF"
        PythonType: "lsst.afw.image.ExposureF"
        StoragePolicy: {
            Storage: "FitsStorage"
            Location: "%(input)/%(inputPathName)"
        }
    }
    skyTile: {
        Type: "SkyMapImage"
        PythonType: "lsst.afw.image.SkyMapImage"
        StoragePolicy: {
            Storage: "BoostStorage"
            Location: "%(input)/%(skyTilePathName)"
        }
}
