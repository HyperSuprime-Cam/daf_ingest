#<?cfg paf policy ?>
# Input.paf
parameters: {
    butler: {
        mapperName: lsst.obs.lsstSim.LsstSimMapper
	mapperPolicy: {
            root: %(input)
        }
    }
    inputItems: {
        isrExposure: {
            datasetType: raw
            datasetId: {
                fromInputDatasets: true
            }
        }
        biasExposure: {
            datasetType: bias
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
        darkExposure: {
            datasetType: dark
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
        flatExposure: {
            datasetType: flat
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
#        fringeExposure: {
#            datasetType: fringe
#            datasetId: {
#                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
#            }
#        }
    }
}
