AdditionalData: "cameraInfo=triggerISREvent.cameraInfo"
inputKeys: {
    inputDatasets: inputDatasets
    jobIdentity: jobIdentity
}
parameters: {
    butler: {
        mapperName: minMapper.MinMapper
	mapperPolicy: {
            root: /lsst/sims/ImSim_data
        }
    }
    inputItems: {
        rawCameraImage: {
            datasetType: raw
            datasetId: {
                fromInputDatasets: true
            }
        }
        biasExposure: {
            datasetType: bias
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
        darkExposure: {
            datasetType: dark
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
        flatExposure: {
            datasetType: flat
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
        fringeExposure: {
            datasetType: fringe
            datasetId: {
                fromJobIdentity: "visit" "snap" "raft" "sensor" "channel"
            }
        }
    }
}
