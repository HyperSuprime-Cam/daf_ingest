#<?cfg paf policy ?>
# CFHT output.paf
parameters: {
    butler: {
        mapperName: lsst.obs.cfht.CfhtMapper
        mapperPolicy: {
            root: %(input)
        }
    }
   outputItems: {
      sources: {
         datasetId: {
            datasetType: source
            fromJobIdentity: "skyTileId"
         }
      }
      badSources: {
         datasetId: {
            datasetType: badSource
            fromJobIdentity: "skyTileId"
         }
      }
      invalidSources: {
         datasetId: {
            datasetType: invalidSource
            fromJobIdentity: "skyTileId"
         }
      }
      goodSourceHistogram: {
         datasetId: {
            datasetType: sourceHist
            fromJobIdentity: "skyTileId"
         }
      }
      badSourceHistogram: {
         datasetId: {
            datasetType: basSourceHist
            fromJobIdentity: "skyTileId"
         }
      }
      sourceClusterAttributes: {
         datasetId: {
            datasetType: object
            fromJobIdentity: "skyTileId"
         }
      }
      badSourceClusterAttributes: {
         datasetId: {
            datasetType: badObject
            fromJobIdentity: "skyTileId"
         }
      }
   }
    
}
