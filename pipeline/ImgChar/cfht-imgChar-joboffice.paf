#<?cfg paf policy ?>
# cfht-imgChar-joboffice.paf

name:  ImgChar
persist.dir:  %(schedroot)s/%(name)s-joboffice

listen: {
    brokerHostName:      lsst8.ncsa.uiuc.edu
    dataReadyEvent:      visitimAvailable
    pipelineEvent:       ImgCharJob
    stopEvent:           JobOfficeStop
}

schedule: {
    trigger: {
        datasetType: visitim
        id: visit
        id: filter
        id: ccd
    }

    job: {
        input: {
            datasetType: visitim
            id: visit
            id: filter
            id: ccd
        }
        output: {
            datasetType: calexp
            id: visit
            id: filter
            id: ccd
        }
        output: {
            datasetType: psf
            id: visit
            id: filter
            id: ccd
        }
#        output: {
#            datasetType: cellSet
#            id: visit
#            id: filter
#            id: ccd
#        }
#        output: { 
#            datasetType: sourceMatchSet
#            id: visit
#            id: filter
#            id: ccd
#        }
    }
}
