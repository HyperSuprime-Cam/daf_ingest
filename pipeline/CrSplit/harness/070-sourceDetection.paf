# sourceDetection.paf
inputKeys: {
   exposure: "diffExposure"
}
outputKeys: {
   positiveDetection: "positiveFootprintSet"
   negativeDetection: "negativeFootprintSet"
   psf: "psf"
}

backgroundPolicy: {
    algorithm:  "NONE"
}

detectionPolicy: {
    minPixels: 1
    nGrow : 0
    thresholdValue: 10.0
    thresholdType: "stdev"
    thresholdPolarity: "both"
}

