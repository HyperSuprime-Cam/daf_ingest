backgroundEstimationStage: {
    inputKeys: {
        exposure: "calibratedExposure"
    }
    outputKeys: {
        exposure: "calibratedExposure"
        background: "background"
    }
    parameters: {
        subtractBackground: true
    }
}

crRejectStage: {
    inputKeys: {
        exposure: "calibratedExposure"
    }
    
    outputKeys: {
        exposure: "calibratedExposure"
    }
    
    parameters: {
        defaultFwhm : 1.0
        keepCRs: False
    }
}
