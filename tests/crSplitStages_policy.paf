#<? cfg paf policy ?>
# 

BackgroundEstimationStage: {
    inputKeys: {
        exposure0: "calibratedExposure0"
        exposure1: "calibratedExposure1"
    }
    outputKeys: {
        exposure0: "calibratedExposure0"
        exposure1: "calibratedExposure1"
        background: None
    }
    parameters: {
        subtractBackground: true
    }
}

CrRejectStage: {
    inputKeys: {
        exposure0: "calibratedExposure0"
        exposure1: "calibratedExposure1"
    }
    
    outputKeys: {
        exposure0: "calibratedExposure0"
        exposure1: "calibratedExposure1"
    }
    
    parameters: {
        defaultFwhm : 1.0
        #keepCRs: true
        keepCRs: false
    }
}

SimpleDiffImStage: {
    inputKeys: {
        exposures: "calibratedExposure0" "calibratedExposure1"
    }
    outputKeys: {
        differenceExposure: "differenceExposure"
    }
    parameters: {
    }
}
